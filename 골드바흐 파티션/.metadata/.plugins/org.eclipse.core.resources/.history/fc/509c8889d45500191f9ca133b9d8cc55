
public class Main {

	public static void main(String[] args) {
		getChe1(10000);
	}
	
	// 에라토스테네스의 체
	private static void getChe1(int num) {
		int[] arr = new int[num+1];
		
		for(int i = 2; i <= num; i++) // 나누는 값 : i
			for(int j = 2; j <= num; j++)
				if(j != i && j%i == 0) // 자신과 같지 않고 딱 나누어떨지면 소수가 아님
					arr[j] = 1; // 소수가 아닌 경우 1을 넣어둔다.
		
		for(int i = 2; i <= num; i++)
			if(arr[i] == 0)
				System.out.print(i + " ");
	}
}
