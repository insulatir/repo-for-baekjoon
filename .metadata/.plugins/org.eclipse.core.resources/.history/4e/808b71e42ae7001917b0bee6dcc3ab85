import java.util.*;
public class Main {
	static int[][] map;
	static int V;

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		V = scan.nextInt();
		int E = scan.nextInt();
		
		map = new int[V+1][V+1];
		for (int i = 1; i < E; i++) {
			int a = scan.nextInt();
			int b = scan.nextInt();
			int c = scan.nextInt();
			
			map[a][b] = c;
		}
		
		
		
		scan.close();
	}

	public static void ployd() {
		for (int k = 1; k <= V)
	}
}
